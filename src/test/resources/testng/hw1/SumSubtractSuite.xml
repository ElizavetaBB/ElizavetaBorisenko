<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sum and subtract tests" verbose="1">
    <test name="Sum">
        <groups>
            <run>
                <include name="sum"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.tc.hw1.SumTest"/>
        </classes>
    </test>
    <test name="Subtract">
        <groups>
            <run>
                <include name="subtract"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.tc.hw1.SubtractTest"/>
        </classes>
    </test>
</suite>
