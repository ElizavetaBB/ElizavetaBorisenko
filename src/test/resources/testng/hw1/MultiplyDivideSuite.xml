<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multiply and divide tests" verbose="1">
    <test name="Multiply">
        <groups>
            <run>
                <include name="multiply"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.tc.hw1.MultiplyTest"/>
        </classes>
    </test>
    <test name="Divide">
        <groups>
            <run>
                <include name="divide"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.tc.hw1.DivideTest"/>
        </classes>
    </test>
</suite>
